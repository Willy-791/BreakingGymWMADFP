
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]- Breaking GYM</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/BreakingGymWebUI.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/Membresia.css" />
    <link rel="stylesheet" href="~/css/SobreNosotros.css" />
    <!-- Font Awesome CDN -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.0/css/all.min.css">
    <!-- icono de la pestaña-->
    <link rel="icon" href="~/img/b1e2817c-8b23-4e7a-9c90-899ee0c1872e.png" type="image/png">
</head>
<body class="sb-nav-fixed">
    <nav class="sb-topnav navbar navbar-expand navbar-dark bg-dark">
        <!-- Navbar Brand-->
        <a class="navbar-brand ps-3" href="@Url.Action("Index", "Home")"> Breaking GYM</a>
        <!-- Sidebar Toggle-->
        <button class="btn btn-link btn-sm order-1 order-lg-0 me-4 me-lg-0" id="sidebarToggle" href="#!"><i class="fas fa-bars" style="color: #ffc107;"></i></button>
        <!-- Navbar Search-->
       @*  <form class="d-none d-md-inline-block form-inline ms-auto me-0 me-md-3 my-2 my-md-0">
            <div class="input-group">
                <input class="form-control" type="text" placeholder="Search for..." aria-label="Search for..." aria-describedby="btnNavbarSearch" />
                <button class="btn btn-primary" id="btnNavbarSearch" type="button"><i class="fas fa-search"></i></button>
            </div>
        </form> *@
        <!-- Navbar-->
        <ul class="navbar-nav ms-auto me-0 me-md-3 my-2 my-md-0">
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" id="navbarDropdown" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false"><i class="fas fa-user fa-fw" style="color: #ffc107;"></i></a>
                <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdown">
                    <li><a class="dropdown-item" href="@Url.Action("Login", "Login")">Iniciar Sesión</a></li>
                    <li><a class="dropdown-item" href="@Url.Action("GuardarUsuario", "Usuario")"> Registrarse</a></li>
        
                </ul>
            </li>
        </ul>
    </nav>
    <div id="layoutSidenav">
        <div id="layoutSidenav_nav">
            <nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
                <div class="sb-sidenav-menu">
                    <div class="nav">
                        <div class="sb-sidenav-menu-heading">Resumen</div>
                        <a class="nav-link" href="@Url.Action("Index", "Home")">
                            <div class="sb-nav-link-icon"><i class="fa-solid fa-house" style="color:#ffc107"></i></div>
                            Inicio
                        </a>
                        <a class="nav-link" href="@Url.Action("SobreNosotros", "Home")">
                            <div class="sb-nav-link-icon"><i class="fas fa-info-circle" style="color: #ffc107;"></i></div>
                            Sobre Nosotros
                        </a>
                        <div class="sb-sidenav-menu-heading">Breaking GYM</div>
                        <a class="nav-link" href="@Url.Action("MostrarSU", "Servicio")">
                            <div class="sb-nav-link-icon"><i class="fa-solid fa-hand-holding-hand" style="color: #ffc107;"></i></div>
                            Servicios
                        </a>
                        <a class="nav-link" href="@Url.Action("Index", "Membresia")">
                            <div class="sb-nav-link-icon"><i class="fa-solid fa-money-check-dollar" style="color: #ffc107;"></i></div>
                            Membresías
                        </a>
                        
                    </div>
                </div>
                <div class="sb-sidenav-footer">
                    <div class="small colorw">Contactanos en nuestras redes:</div>
                    <a class="iconos" href="https://web.whatsapp.com/" target="_blank" rel="WhatsApp">
                        <i class="fa-brands fa-whatsapp" style="color: #06402B;"></i>
                    </a>
                    <a class="iconos" href="https://mail.google.com/" target="_blank" rel="Gmail">
                        <i class="fa-solid fa-envelope iconG" style="color: #D14836;"></i>
                    </a>
                   
                    <a class="iconos" href="https://www.instagram.com/" target="_blank" rel="Instagram">
                        <i class="fa-brands fa-instagram" style="color: #d6249f;"></i>
                    </a>
                    <a class="iconos" href="https://es.linkedin.com/" target="_blank" rel="Linkedin">
                        <i class="fa-brands fa-linkedin" style="color: #0000FF;"></i>
                    </a>
                </div>
            </nav>

        </div>
        <div id="layoutSidenav_content">
            <main>
                <div class="container-fluid px-4">
                    @RenderBody()
                </div>
            </main>
          
        </div>
    </div>
    <footer class="py-4 bg-light mt-auto">
        <div class="container-fluid px-4">
            <div class="d-flex align-items-center justify-content-between small">
                <div class="text-muted">Copyright &copy; Your Website 2023</div>
                <div>
                    
                </div>
            </div>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/scripts.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
